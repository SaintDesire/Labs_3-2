generator client {
  provider = "prisma-client-js"
}


datasource db {
  provider = "sqlserver"
  url      = "sqlserver://localhost:1433;database=KNI;user=student;password=fitfit;encrypt=true;trustServerCertificate=true;"
}

model AUDITORIUM {
  AUDITORIUM                                                  String          @id(map: "PK__AUDITORI__537260102207767D") @db.NVarChar(10)
  AUDITORIUM_NAME                                             String?         @db.NVarChar(255)
  AUDITORIUM_CAPACITY                                         Int?
  AUDITORIUM_TYPE                                             String          @db.NVarChar(10)
  AUDITORIUM_TYPE_AUDITORIUM_AUDITORIUM_TYPEToAUDITORIUM_TYPE AUDITORIUM_TYPE @relation("AUDITORIUM_AUDITORIUM_TYPEToAUDITORIUM_TYPE", fields: [AUDITORIUM_TYPE], references: [AUDITORIUM_TYPE], onUpdate: NoAction, map: "FK__AUDITORIU__AUDIT__236943A5")
}

model AUDITORIUM_TYPE {
  AUDITORIUM_TYPE                                        String       @id(map: "AUDITORIUM_TYPE_PK") @db.NVarChar(10)
  AUDITORIUM_TYPENAME                                    String       @db.NVarChar(255)
  AUDITORIUM_AUDITORIUM_AUDITORIUM_TYPEToAUDITORIUM_TYPE AUDITORIUM[] @relation("AUDITORIUM_AUDITORIUM_TYPEToAUDITORIUM_TYPE")
}

model FACULTY {
  FACULTY                        String   @id(map: "PK_FACULTY") @db.NVarChar(10)
  FACULTY_NAME                   String   @db.NVarChar(255)
  PULPIT_PULPIT_FACULTYToFACULTY PULPIT[] @relation("PULPIT_FACULTYToFACULTY")
}

model PULPIT {
  PULPIT                          String    @id(map: "PK_PULPIT") @db.NVarChar(10)
  PULPIT_NAME                     String?   @db.NVarChar(255)
  FACULTY                         String    @db.NVarChar(10)
  FACULTY_PULPIT_FACULTYToFACULTY FACULTY   @relation("PULPIT_FACULTYToFACULTY", fields: [FACULTY], references: [FACULTY], onUpdate: NoAction, map: "FK__PULPIT__FACULTY__208CD6FA")
  SUBJECT_SUBJECT_PULPITToPULPIT  SUBJECT[] @relation("SUBJECT_PULPITToPULPIT")
  TEACHER_TEACHER_PULPITToPULPIT  TEACHER[] @relation("TEACHER_PULPITToPULPIT")
}

model SUBJECT {
  SUBJECT                       String @id(map: "PK_SUBJECT") @db.NVarChar(10)
  SUBJECT_NAME                  String @db.NVarChar(255)
  PULPIT                        String @db.NVarChar(10)
  PULPIT_SUBJECT_PULPITToPULPIT PULPIT @relation("SUBJECT_PULPITToPULPIT", fields: [PULPIT], references: [PULPIT], onUpdate: NoAction, map: "FK__SUBJECT__PULPIT__22751F6C")
}

model TEACHER {
  TEACHER                       String  @id(map: "PK_TEACHER") @db.NVarChar(10)
  TEACHER_NAME                  String? @db.NVarChar(255)
  PULPIT                        String  @db.NVarChar(10)
  PULPIT_TEACHER_PULPITToPULPIT PULPIT  @relation("TEACHER_PULPITToPULPIT", fields: [PULPIT], references: [PULPIT], onUpdate: NoAction, map: "FK__TEACHER__PULPIT__2180FB33")
}
